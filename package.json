{
  "name": "@electron/docs-parser",
  "version": "0.0.0-development",
  "description": "Parse Electron documentation into a machine readable JSON file",
  "main": "dist/index.js",
  "author": "Samuel Attard",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "prettier:check": "prettier --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "jest"
  },
  "bin": {
    "electron-docs-linter": "./dist/bin.js",
    "electron-docs-parser": "./dist/bin.js"
  },
  "devDependencies": {
    "@continous-auth/semantic-release-npm": "1.0.3",
    "@types/chai": "^4.1.7",
    "@types/fs-extra": "^5.0.5",
    "@types/jest": "^24.0.11",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/minimist": "^1.2.0",
    "@types/pretty-ms": "^4.0.0",
    "jest": "^24.7.1",
    "prettier": "^1.17.0",
    "semantic-release": "^15.13.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "@types/markdown-it": "^0.0.7",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "fs-extra": "^7.0.1",
    "lodash.camelcase": "^4.3.0",
    "markdown-it": "^8.4.2",
    "minimist": "^1.2.0",
    "ora": "^3.4.0",
    "pretty-ms": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electron/docs-parser.git"
  },
  "keywords": [
    "electron",
    "docs",
    "documentation",
    "api",
    "typescript",
    ".d.ts",
    "definitions"
  ],
  "bugs": {
    "url": "https://github.com/electron/docs-parser/issues"
  },
  "homepage": "https://github.com/electron/docs-parser#readme"
}